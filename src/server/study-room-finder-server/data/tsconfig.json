{
    "extends": "astro/tsconfigs/strict",
    "include": [".astro/types.d.ts", "**/*", "src", "astro.config.mjs"],
    "exclude": ["dist"],
    "compilerOptions": {
        "jsx": "react-jsx",
        "jsxImportSource": "react",
        "baseUrl": ".",
        "paths": {
            "@/*": ["./src/*"]
        },
        "types": ["astro/client"]
    }
}
